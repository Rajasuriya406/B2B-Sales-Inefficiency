##Load & filter correct data

import pandas as pd
import matplotlib.pyplot as plt


df = pd.read_csv("sales_pipeline.csv")

won = df[df["deal_stage"] == "Won"].copy()
won = won[won["close_value"].notna()]

df.head()
df.describe()
df.size()

#Distribution
won["close_value"].describe(percentiles=[0.25, 0.5, 0.75])


#Outlier detection (IQR)
q1 = won["close_value"].quantile(0.25)
q3 = won["close_value"].quantile(0.75)
iqr = q3 - q1

lower = q1 - 1.5 * iqr
upper = q3 + 1.5 * iqr

outliers = won[(won["close_value"] < lower) | (won["close_value"] > upper)]

outliers[["sales_agent", "product", "close_value"]].sort_values("close_value", ascending=False)

##Histogram
import matplotlib.pyplot as plt

plt.figure()
plt.hist(won["close_value"], bins=30)
plt.xlabel("Close Value")
plt.ylabel("Frequency")
plt.title("Distribution of Close Value (Won Deals)")
plt.show()


##Boxplot
plt.figure(figsize=(6,4))
plt.boxplot(df['deal_size'], vert=False)
plt.xlabel("Deal Size (USD)")
plt.title("Deal Size Outliers (IQR Method)")
plt.show()

